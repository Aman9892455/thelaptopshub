    <!-- CTA Section -->
    <section class="cta">
        <div class="container">
            <h2>Ready to work with us?</h2>
            <p>Get in touch with our team today to discuss how we can help you achieve your goals.</p>
            <a href="/contact" class="btn">Contact Us</a>
